<%= form_for [:admin, @beer], multipart: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :state %>
  <%= f.text_field :state %><br>
  <%= f.label :description %>
  <%= f.text_field :description %><br>
  <%= f.label :price %>
  <%= f.text_field :price %><br>
  <%= f.label :attachment %>
  <%= f.file_field :attachment %><br>
  <p>Categories:</p>
  <%= f.collection_check_boxes(:categories, Category.all, :id, :name) %>
  <%= f.submit %>
<% end %>