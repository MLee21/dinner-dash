<%= form_for [:admin, @beer], multipart: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :attachment %>
  <%= f.file_field :attachment %>
  <%= f.submit %>
<% end %>